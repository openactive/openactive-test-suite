    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.6.0",
    "@types/yargs": "^15.0.9",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": "18.17.1"
